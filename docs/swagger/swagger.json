    "host": "192.168.1.88:8080",
    "basePath": "/",
    "paths": {
        "/login/": {
            "post": {
                "description": "用户登陆接口",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Swagger"
                ],
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户名",
                        "name": "name",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "密码",
                        "name": "password",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/helper.JsonObject"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "helper.JsonObject": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "* 状态码",
                    "type": "string"
                },
                "content": {
                    "description": "* 内容体",
                    "type": "object"
                },
                "message": {
                    "description": "* 消息",
                    "type": "string"
                }
            }
        }
    }
